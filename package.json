{
    "name": "vscode-android-repo",
    "publisher": "microhobby",
    "icon": "assets/img/logo.png",
    "author": {
        "name": "Matheus Castello",
        "email": "matheus@castello.eng.br"
    },
    "repository": {
        "url": ""
    },
    "displayName": "Repo Tool Manifest",
    "description": "Extension to help with Repo Manifest workspaces",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.83.0"
    },
    "categories": [
        "Other",
        "SCM Providers"
    ],
    "activationEvents": [
        "onLanguage:xml"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "vscode-android-repo.helloWorld",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "eslint src --ext ts",
        "pack": "vsce package"
    },
    "devDependencies": {
        "@types/vscode": "^1.83.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/jsdom": "^21.1.4",
        "@typescript-eslint/eslint-plugin": "^6.4.1",
        "@typescript-eslint/parser": "^6.4.1",
        "eslint": "^8.47.0",
        "glob": "^10.3.3",
        "mocha": "^10.2.0",
        "typescript": "^5.1.6",
        "@vscode/test-electron": "^2.3.4",
        "eslint-plugin-spellcheck": "^0.0.20",
        "eslint-config-standard-with-typescript": "^39.1.1"
    },
    "dependencies": {
        "jsdom": "^22.1.0"
    }
}
